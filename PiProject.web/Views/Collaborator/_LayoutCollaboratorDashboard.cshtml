<!DOCTYPE html>

<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<title>@ViewBag.Title - My ASP.NET Application</title>

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">




	@Styles.Render("~/Content/css")
	@Styles.Render("~/content/CollaboratorLayout.css")
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>
	<div class="top_bar">
		<div class="container">
			
			@Html.ActionLink("Deconnect", "Deconnect", null, new { @class = "btn btn-dark login" })
			
		</div>
	</div>
	
	<div id="nav_holder">
		<ul id="default" class="nav nav-pills">

			<li class="nav-item">
				@Html.ActionLink("aceuuil", "Dashboard", null, new { @class = "nav-link" })
			</li>
			<li class="nav-nav-item dropdown">
				<a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"> Display tests</a>
				<div class="dropdown-menu">

					@Html.ActionLink("360 Tests", "Tests360", null, new { @class = "dropdown-item" })
					@Html.ActionLink("Auto Tests", "AutoTests", null, new { @class = "dropdown-item" })

				</div>
				
			
			</li>

			<li class="nav-item">
				<a class="nav-link " href="#"> myTeam </a>
			</li>
			<li class="nav-item">
				@Html.ActionLink("display Warnings", "DisplayWarnings", null, new { @class = "nav-link active" })
				
			</li>
			<li class="nav-item">
				<a class="nav-link " href="#"> Missions </a>
			</li>
			<li class="nav-item">
				<a class="nav-link " href="#"> Formations </a>
			</li>
		
			

		</ul>

		
	</div>
	


	




    <div>

        @RenderBody()
    </div>


	<footer>
		<div class="footer_holder">
			<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p> 
		</div>
		
	</footer>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

	
	
	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	
	



	@RenderSection("scripts", required: false)
</body>
</html>
